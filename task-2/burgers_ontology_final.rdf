<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/amari/ontologies/2024/11/burgers/"
     xml:base="http://www.semanticweb.org/amari/ontologies/2024/11/burgers/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers">
        <rdfs:comment xml:lang="en">This is an ontology about burgers.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_bun -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_bun">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_ingredrient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_bun_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Bun"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_ingredrient -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_ingredrient">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_ingredient_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_patty -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_patty">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_ingredrient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_patty_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Patty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_ingredrient"/>
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_topping_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Topping"/>
        <rdfs:comment xml:lang="en">has_topping is the inverse of is_topping_of (which is inferred by the HermiT reasoner)</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_bun_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_bun_of">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_ingredient_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_ingredient_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_ingredient_of">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_itself -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_itself">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ReflexiveProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_patty_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_patty_of">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_ingredient_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_topping_of -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_topping_of">
        <rdfs:subPropertyOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#is_ingredient_of"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#BBQ_Burger -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#BBQ_Burger">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Named_Burger"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#BBQ_Topping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Bacon_Topping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheddar_Topping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_patty"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Beef_Patty"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Chicken_Patty"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">BBQ burger with either beef or chicken patty, some BBQ sauce, bacon and cheddar cheese.

There can be other things on it as well.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#BBQ_Topping -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#BBQ_Topping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Sauce_Topping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Bacon_Burger -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Bacon_Burger">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Bacon_Topping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">Any burger with some bacon on it.

There can be other things on the burger, as long as there is some bacon on it, it&apos;s a bacon burger.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Bacon_Topping -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Bacon_Topping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Meat_Topping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Beef_Patty -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Beef_Patty">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Patty"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Boring_Burger -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Boring_Burger">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
                    <owl:Class>
                        <owl:complementOf>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Sauce_Topping"/>
                            </owl:Restriction>
                        </owl:complementOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_bun"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#PlainWhite_Bun"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
        <rdfs:comment xml:lang="en">Any burger with a plain white bun and no sauce.

There can be other toppings on it.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_bun"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Bun"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_patty"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Patty"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Bun -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Bun"/>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Patty -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Patty"/>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Topping -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Topping"/>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheap_Burger -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheap_Burger">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
                    <owl:Class>
                        <owl:complementOf>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Meat_Topping"/>
                            </owl:Restriction>
                        </owl:complementOf>
                    </owl:Class>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_patty"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:unionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Beef_Patty"/>
                                    <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Chicken_Patty"/>
                                </owl:unionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">2</owl:maxQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Sauce_Topping"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:maxQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Vegetable_Topping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
        <rdfs:comment xml:lang="en">Cheap burgers have a beef or chicken patty, max 2 sauces, no meat toppings, max 3 vegetables. Can contain anything else.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheddar_Topping -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheddar_Topping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheese_Topping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheese_Topping -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheese_Topping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Topping"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheeseburger -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheeseburger">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Cheese_Topping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment xml:lang="en">Any burger with some cheese on it.

There can be other things on the burger, as long as there is some cheese on it, it&apos;s a cheeseburger.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Chicken_Burger -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Chicken_Burger">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_patty"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Chicken_Patty"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger"/>
        <rdfs:comment xml:lang="en">Any burger with a chicken patty.

Burger has exactly 1 patty according to the superclass Burger definition, which for the chicken burger is the chicken patty. Anything else can be on it, as long as the patty is chicken, it&apos;s a chicken burger.</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Chicken_Patty -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Chicken_Patty">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Burger_Patty"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Classic_Burger -->

    <owl:Class rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Classic_Burger">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Named_Burger"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_bun"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#SesameSeed_Bun"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_patty"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Beef_Patty"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Lettuce_Topping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Pickles_Topping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Tomato_Topping"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#has_topping"/>
                <owl:allValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Lettuce_Topping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Pickles_Topping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Sauce_Topping"/>
                            <rdf:Description rdf:about="http://www.semanticweb.org/amari/ontologies/2024/11/burgers#Tomato_Topping"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:allValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
